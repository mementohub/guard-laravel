{
    "name": "imemento/guard-laravel",
    "description": "Our custom guard for Laravel",
    "homepage": "https://gitlab.com/imemento/composer/packages/guard-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php" : "^5.6|^7.0",
        "illuminate/contracts": "~5.1.0|~5.2.0|~5.3.0|~5.4.0",
        "illuminate/support": "~5.1.0|~5.2.0|~5.3.0|~5.4.0",
        "imemento/jwt": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.7.5"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Guard\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Guard\\Laravel\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "iMemento\\Guard\\Laravel\\AuthServiceProvider"
            ]
        }
    }
}
