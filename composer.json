{
    "name": "imemento/guard-laravel",
    "description": "Our custom guard for Laravel",
    "homepage": "https://github.com/mementohub/guard-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": ">=7.2.5 <7.4",
        "laravel/framework": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "imemento/sdk-auth": "^0.1.0",
        "imemento/jwt": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "orchestra/testbench": "^3.8 || ^4.0 || ^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Guard\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Guard\\Laravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "iMemento\\Guard\\Laravel\\AuthServiceProvider"
            ]
        }
    }
}
